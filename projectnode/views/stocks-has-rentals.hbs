<h1>Stocks Has Rentals</h1>
<table>
    <thead>
        <tr>
            <th>Stock ID</th>
            <th>Rental ID</th>
            {{!-- <th>Actions</th> --}}
        </tr>
    </thead>
    <tbody>
        {{#each stocksHasRentals}}
        <tr>
            <td>{{this.stockID}}</td>
            <td>{{this.rentalID}}</td>
            {{!-- <td></td> --}}
        </tr>
        {{/each}}
    </tbody>
</table>

